import numpy as np
#diberikan matriks:
M1 =np.array([[1, 3, 2],[4, 1, 6]])
M2 =np.array([[2, 1, 3],[0, 5, 2]])
#penjumlahan matriks
M_tambah = M1 + M2
print("pemjumlahan matriks:")
print(M_tambah)

#pengurangan matriks
M_kurang = M1-M2
print("pengurangan matris:")
print(M_kurang)

#transpose
M_transpose = M1.T
print("matriks transpose:")
print(M_transpose)

#perkalian matriks dengan transpose
M_perkalian = M1 @ M2.T
print("perkalian matriks:")
print(M_perkalian)
